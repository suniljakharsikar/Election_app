<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/tools">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnSwipe
            motion:touchAnchorId="@+id/cardView3"
            motion:dragDirection="dragDown" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/cardView3"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp"
            motion:layout_constraintTop_toTopOf="@+id/guideline5"
            android:layout_marginLeft="16dp"
            app:contentPaddingBottom="130dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="24dp"
            android:layout_marginRight="16dp" />
        <Constraint
            android:id="@+id/profile_image"
            android:elevation="2dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            android:layout_height="120dp"
            motion:layout_constraintBottom_toTopOf="@+id/cardView3"
            motion:layout_constraintTop_toTopOf="@+id/cardView3"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/textView10"
            android:elevation="2dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/textView32" />
        <Constraint
            android:id="@+id/textView4"
            android:elevation="2dp"
            android:visibility="visible"

            motion:layout_constraintEnd_toEndOf="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView10"
            motion:layout_constraintStart_toStartOf="@+id/textView10" />
        <Constraint
            android:id="@+id/textView32"
            android:elevation="2dp"
            android:visibility="visible"

            motion:layout_constraintEnd_toEndOf="@+id/profile_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@+id/profile_image"
            android:layout_marginTop="4dp"
            motion:layout_constraintTop_toBottomOf="@+id/profile_image" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/cardView3"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp"
            motion:layout_constraintTop_toTopOf="@+id/guideline5"
            android:layout_marginLeft="16dp"
            app:contentPaddingBottom="0dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="24dp"
            android:layout_marginRight="16dp"
            motion:transitionEasing="standard" />
        <Constraint
            android:id="@+id/profile_image"
            android:elevation="2dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="80dp"
            android:layout_height="80dp"
            motion:layout_constraintBottom_toTopOf="@+id/cardView3"
            motion:layout_constraintTop_toTopOf="@+id/cardView3"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/textView10"
            android:elevation="2dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"

            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/textView32" />
        <Constraint
            android:id="@+id/textView4"
            android:elevation="2dp"
            motion:layout_constraintEnd_toEndOf="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"
            motion:layout_constraintTop_toBottomOf="@+id/textView10"
            motion:layout_constraintStart_toStartOf="@+id/textView10" />
        <Constraint
            android:id="@+id/textView32"
            android:elevation="2dp"
            motion:layout_constraintEnd_toEndOf="@+id/profile_image"
            android:layout_width="wrap_content"
            android:visibility="gone"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@+id/profile_image"
            android:layout_marginTop="4dp"
            motion:layout_constraintTop_toBottomOf="@+id/profile_image" />

    </ConstraintSet>
</MotionScene>