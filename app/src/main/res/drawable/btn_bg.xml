<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="32dip" />
            <stroke android:width="1dip" android:color="#FFFFFF" />
            <gradient android:angle="-90" android:endColor="#4F5C91" android:startColor="#4F5C91" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="32dip" />
            <stroke android:width="1dip" android:color="#FFFFFF" />
            <solid android:color="#001364" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="32dip" />
            <stroke android:width="1dip" android:color="#FFFFFF" />
            <gradient android:angle="-90" android:endColor="#001364" android:startColor="#001364" />
        </shape>
    </item>
</selector>